#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules/astro/node_modules:/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/kharipowell/Developer/iHubSiteAstro copy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@1.21.7_rollup@4.44.2_typescript@5.8.3_yaml@2.8.0/node_modules/astro/astro.js" "$@"
fi
